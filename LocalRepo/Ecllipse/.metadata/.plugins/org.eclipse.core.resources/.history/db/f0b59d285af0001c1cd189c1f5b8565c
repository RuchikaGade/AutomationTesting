package program;

public class Addition {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
int a=25;
int b=10;
int sum=a+b;
System.out.println(sum);
	}

}
public class Subtraction {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
int a=250;
int b=100;
int sub=a-b;
System.out.println(sub);
	}

}
