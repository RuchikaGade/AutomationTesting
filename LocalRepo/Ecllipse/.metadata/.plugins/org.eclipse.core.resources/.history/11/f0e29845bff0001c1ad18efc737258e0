package program;

public class ForLoop {

	public static void main(String[] args) {
	for (int i=0;i<10;++i) {
		System.out.println("####### Hello i:"+i);
		for(int j=0;j<10;j++) {
			System.out.println("########## Hii j:"+j);
			for(int k=0;k<10;k++) {
				System.out.println("#######bye k:"+k);
			}
		}
	}

	}

}
